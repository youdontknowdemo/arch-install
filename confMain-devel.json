  packages: [
    arch-install-scripts,
    bash,
    binutils,
    ccache,
    chromium,
    coreutils,
    efibootmgr,
    fakeroot,
    git,
    gtk-update-icon-cache,
    linux-headers,
    linux-lts-headers,
    linux-zen-headers,
    meson,
    nano,
    ninja,
    procs,
    rsync,
    usbutils,
    zlib,
    zstd,
    wget,
    xdg-utils,
    xf86-input-libinput
